{
    "name": "@tleef/code",
    "description": "assertion library",
    "version": "5.2.0-r.1",
    "repository": "git://github.com/tleef/code",
    "main": "lib/index.js",
    "keywords": [
        "test",
        "expect",
        "assertion"
    ],
    "engines": {
        "node": ">=6.10"
    },
    "dependencies": {
        "@babel/runtime": "^7.0.0-beta.56",
        "@tleef/hoek": "^5.0.3"
    },
    "devDependencies": {
        "@babel/cli": "^7.0.0-beta.56",
        "@babel/core": "^7.0.0-beta.56",
        "@babel/plugin-transform-runtime": "^7.0.0-beta.56",
        "@babel/preset-env": "^7.0.0-beta.56",
        "@babel/register": "^7.0.0-beta.56",
        "coveralls": "^3.0.2",
        "markdown-toc": "1.1.x",
        "mocha": "^5.2.0",
        "nyc": "^12.0.2"
    },
    "scripts": {
        "build": "babel src --out-dir lib",
        "test": "mocha --require @babel/register",
        "test:coverage": "nyc --reporter lcovonly --reporter text yarn test",
        "toc": "node generate-api-toc.js",
        "version": "npm run toc && git add API.md",
        "prepare": "yarn build"
    },
